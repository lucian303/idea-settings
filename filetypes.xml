<application>
  <component name="CommunityFileTypes" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;" />
    <extensionMap>
      <mapping ext="jspx" type="XML" />
      <mapping ext="tagx" type="XML" />
    </extensionMap>
  </component>
  <component name="FileTypeManager" version="18">
    <extensionMap>
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping pattern="fff" type="PLAIN_TEXT" />
      <mapping ext="port" type="PLAIN_TEXT" />
      <mapping ext="scss" type="PLAIN_TEXT" />
      <removed_mapping ext="apk" approved="true" type="ARCHIVE" />
      <removed_mapping ext="psd" approved="true" type="Image" />
      <mapping pattern=".gitignore_global" type="Git file" />
    </extensionMap>
  </component>
</application>